<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro"> 

<xacro:include filename="inertial_macros.xacro"/>

<material name="white">
    <color rgba="1 1 1 1" />
</material>

<material name="orange">
    <color rgba="1 0.3 0.1 1" />
</material>

<material name="blue">
    <color rgba="0.2 0.2 1 1" />
</material>

<material name="black">
    <color rgba="0 0 0 1" />
</material>

<material name="yellow">
    <color rgba="1 1 0 0" />
</material>

<material name="green">
    <color rgba="0 1 0 1" />
</material>

<link name="base_link">

</link>


<joint name="chassis_wheel_joint" type="fixed">
    <parent link="base_link"/>
    <child link="base_chassis_wheel"/>
    <origin xyz="0 0 0"/> 
</joint>

<link name="base_chassis_wheel">
    <visual>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <cylinder radius="0.15" length="0.02"/>
        </geometry>
        <material name="orange"/>
    </visual>
    <collision>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <geometry>
            <cylinder radius="0.15" length="0.02"/>
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.5" length="0.02" radius="0.15">
            <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
    
</link>

<gazebo reference="base_chassis_wheel">
    <material>Gazebo/Orange</material>
</gazebo>

<joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="0 -0.169 0" rpy="1.57 0 0"/>
    <axis xyz="0 0 -1"/>
</joint>

<link name="right_wheel">
    <visual>    
        <geometry>
            <cylinder radius="0.0425" length="0.048"/>
        </geometry>
        <material name="green"/>
    </visual>
    <collision>    
        <geometry>
            <cylinder radius="0.0425" length="0.048"/>
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.1" length="0.048" radius="0.0425">
            <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
</link>

<gazebo reference="right_wheel">
    <material>Gazebo/Green</material>
</gazebo>

<joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="0 0.169 0" rpy="-1.57 0 0"/>
    <axis xyz="0 0 1"/>
</joint>

<link name="left_wheel">
    <visual>    
        <geometry>
            <cylinder radius="0.0425" length="0.048"/>
        </geometry>
        <material name="green"/>
    </visual>
    <collision>    
        <geometry>
            <cylinder radius="0.0425" length="0.048"/>
        </geometry>
    </collision>
    <xacro:inertial_cylinder mass="0.1" length="0.048" radius="0.0425">
            <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_cylinder>
</link>

<gazebo reference="left_wheel">
    <material>Gazebo/Green</material>
</gazebo>

<joint name="caster_wheel_front_joint" type="continuous">
    <parent link="base_link"/>
    <child link="caster_wheel_front"/>
    <origin xyz="0.12 0 -0.02125"  rpy="0 0 0"/>
    <axis xyz="0 1 1"/>
</joint>

<link name="caster_wheel_front">
    <visual>
        <geometry>
            <sphere radius="0.02125"/>
        </geometry>
        <material name="white"/>
    </visual>
    <collision>
        <geometry>
            <sphere radius="0.02125"/>
        </geometry>
    </collision>
    <xacro:inertial_sphere mass="0.1" radius="0.02125">
            <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_sphere>
</link>

<gazebo reference="caster_wheel_front">
    <material>Gazebo/White</material>
</gazebo>

<joint name="caster_wheel_back_joint" type="continuous">
    <parent link="base_link"/>
    <child link="caster_wheel_back"/>
    <origin xyz="-0.12 0 -0.02125" rpy="0 0 0"/>
    <axis xyz="0 1 1"/>
</joint>

<link name="caster_wheel_back">
    <visual>
        <geometry>
            <sphere radius="0.02125"/>
        </geometry>
        <material name="white"/>
    </visual>
    <collision>
        <geometry>
            <sphere radius="0.02125"/>
        </geometry>
    </collision>
    <xacro:inertial_sphere mass="0.1" radius="0.02125">
            <origin xyz="0 0 0" rpy="0 0 0"/>
    </xacro:inertial_sphere>
</link>

<gazebo reference="caster_wheel_back">
    <material>Gazebo/White</material>
</gazebo>

